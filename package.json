    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "history": "^4.9.0",
    "morgan": "^1.9.1",
    "npm-merge-driver": "^2.3.5",
    "passport": "^0.4.0",
    "passport-google-oauth": "^2.0.0",
    "pg": "^7.11.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-geolocated": "^2.5.0",
    "react-google-maps": "^9.4.5",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "recompose": "^0.30.0",
